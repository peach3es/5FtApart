{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/(page)/mybroker/page.tsx",
    "components/Navbar/navbar.jsx",
    "components/Search/searchoptions.js",
    "backend/database/conn.js",
    "backend/database/controller.js",
    "app/model/property.js",
    "app/model/user.js",
    "app/api/auth/[...nextauth]/route.js",
    "app/(page)/(dropdown)/calculator/page.js"
  ],
  "exclude": ["node_modules"]
}
